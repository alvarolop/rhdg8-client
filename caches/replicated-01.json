{
    "replicated-cache": {
        "state-transfer": {
            "enabled": true,
            "await-initial-transfer": true
        },
        "mode": "SYNC",
        "owners": 1,
        "partition-handling": {
            "when-split": "ALLOW_READ_WRITES",
            "merge-policy": "REMOVE_ALL"
        },
        "transaction": {
            "mode": "NONE"
        },
        "encoding": {
            "media-type": "application/x-protostream"
        },
        "memory": {
            "max-size": "100MB",
            "max-count": -1,
            "when-full": "REMOVE",
            "storage": "HEAP"
        },
        "expiration": {
            "max-idle": -1,
            "lifespan": -1,
            "interval": 60000
        },
        "statistics": true
    }
}
  