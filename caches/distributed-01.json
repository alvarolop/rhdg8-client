{
    "distributed-cache": {
        "encoding": {
            "key": {
              "media-type": "application/x-protostream"
            },
            "value": {
              "media-type": "application/x-protostream"
            }
          },
        "expiration": {
            "max-idle": -1,
            "lifespan": -1,
            "interval": 60000
        },
        "indexing": {
            "enabled": true,
            "key-transformers": [],
            "indexed-entities": [],
            "property": ""
        },
        "memory": {
            "storage": "HEAP"
        },
        "mode": "SYNC",
        "owners": 2,
        "partition-handling": {
            "when-split": "ALLOW_READ_WRITES",
            "merge-policy": "REMOVE_ALL"
        },
        "state-transfer": {
            "enabled": false,
            "await-initial-transfer": false
        },
        "statistics": true,
        "transaction": {
            "mode": "NONE"
        }
    }
}
  